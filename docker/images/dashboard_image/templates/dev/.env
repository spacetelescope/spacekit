APP="cal"
VERSION="dev"
TAG="dash-${APP}-${VERSION}"
DOCKER_REPO="spacetelescope/spacekit"
DOCKER_IMAGE="${DOCKER_REPO}:${TAG}"
SPACEKIT_VERSION="0.4.1"
SRC="git" # pkg, s3, git, file
COLLECTION="calcloud" # bucketname, repo url, or local path
DATASETS="2022-02-14,2021-11-04,2021-10-28"
PFX="archive" # for s3 this is the folder prefix
SPACEKIT_DATA=/home/developer # top-level dir where "data" folder is created
CONTAINER_MODE="-it" # -d for detached, -it for interactive
MOUNTS=1 # >0 will bind mount the below source and dest paths
SOURCEDATA="$(pwd)/docker/data"
DESTDATA="/home/developer/data"
HOSTNAME="localhost"
IPADDRESS=0.0.0.0
NAME="spkt-dash-${APP}-${VERSION}"
EPCOMMAND="python -m spacekit.dashboard.${APP}.index"
